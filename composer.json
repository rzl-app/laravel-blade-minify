{
  "authors": [
    {
      "email": "rizalvindwiky1998@gmail.com",
      "name": "Rizalvin Dwiky",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "RzlApp\\BladeMinify\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RzlApp\\BladeMinify\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "description": "A Laravel package to seamlessly minify Blade View HTML output, enhancing page speed and overall user experience.",
  "extra": {
    "laravel": {
      "providers": [
        "RzlApp\\BladeMinify\\Providers\\RzlBladeMinifyServiceProvider",
        "RzlApp\\BladeMinify\\Providers\\RzlIgnoreMinifyBladeServiceProvider"
      ]
    }
  },
  "homepage": "https://github.com/rzl-app/laravel-blade-minify",
  "keywords": [
    "rzl-app",
    "html-minify",
    "blade-minify",
    "laravel-minify",
    "laravel-blade-minify",
    "laravel",
    "blade",
    "minify",
    "html",
    "performance"
  ],
  "license": "MIT",
  "minimum-stability": "stable",
  "name": "rzl-app/laravel-blade-minify",
  "require": {
    "illuminate/support": "^10.0|^11.0|^12.0",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "type": "library"
}